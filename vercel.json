{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/react"
    },
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/horoscope",
      "dest": "backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}
